{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },

      "dev": {
        "cache": false
      },
      "start": {
        "dependsOn": ["^start"]
      },
      "install": {
        "dependsOn": ["^install"]
      },
      "preview": {
        "dependsOn": ["^preview"]
      },
      "lint": {
        "dependsOn": ["^lint"]
      }
    }
  }